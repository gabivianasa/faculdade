<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.1" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_1.xsd">
    <servlet>
        <servlet-name>CadastroLoginServlet</servlet-name>
        <servlet-class>br.com.login.CadastroLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginControllers</servlet-name>
        <servlet-class>LoginControllers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlteraCadastroLoginServlet</servlet-name>
        <servlet-class>br.com.login.AlteraCadastroLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApagarUsuario</servlet-name>
        <servlet-class>br.com.login.ApagarUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastroLoginServlet</servlet-name>
        <url-pattern>/CadastroLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginControllers</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlteraCadastroLoginServlet</servlet-name>
        <url-pattern>/AlteraCadastroLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApagarUsuario</servlet-name>
        <url-pattern>/ApagarUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
